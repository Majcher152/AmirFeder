
@model IEnumerable<string>

<a class="btn btn-block btn-light" asp-action="List" asp-controller="Product" asp-route-category="">
    Strona główna
</a>

@foreach (string category in Model)
{
    <a class="btn btn-block btn-light"
           asp-action="List" asp-controller="Product" asp-route-category="@category">
            @category
        </a>
}
